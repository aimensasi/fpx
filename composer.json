{
    "name": "aimensasi/fpx",
    "description": "A laravel implementation for FPX payment service",
    "keywords": [
        "aimensasi",
        "FPX"
    ],
    "homepage": "https://github.com/aimensasi/fpx",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "AIMEN.S.A.SASI",
            "email": "aimensasi@icloud.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
		"nesbot/carbon": "^2.48.0",
		"laravelcollective/html": "^6.2",
		"monarobase/country-list": "^3.2"
    },
    "require-dev": {
			"orchestra/testbench": "^6.0",
			"phpunit/phpunit": "^9.3.3"
		},
    "autoload": {
        "psr-4": {
            "Aimensasi\\FPX\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aimensasi\\FPX\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aimensasi\\FPX\\FPXServiceProvider"
            ]
        }
    }
}
